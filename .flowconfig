[ignore]
<PROJECT_ROOT>./config/.*
<PROJECT_ROOT>./node_modules/.*
<PROJECT_ROOT>./public/.*
<PROJECT_ROOT>./scripts/.*

[include]

[libs]

[lints]

[options]
; Extensions
module.file_ext=.type

module.name_mapper='^@assets' ->'<PROJECT_ROOT>/src/assets'
module.name_mapper='^@components' ->'<PROJECT_ROOT>/src/components'
module.name_mapper='^@containers' ->'<PROJECT_ROOT>/src/containers'
module.name_mapper='^@entities' ->'<PROJECT_ROOT>/src/entities'
module.name_mapper='^@services' ->'<PROJECT_ROOT>/src/services'
module.name_mapper='^@store' ->'<PROJECT_ROOT>/src/store'
module.name_mapper='^@styles' ->'<PROJECT_ROOT>/src/styles'
module.name_mapper='^@views' ->'<PROJECT_ROOT>/src/views'
module.name_mapper='^@utils' ->'<PROJECT_ROOT>/src/utils'

[strict]
